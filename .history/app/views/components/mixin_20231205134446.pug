mixin form_items( $text_span,$nameType, $textplace)
		div(class= "inputBox")|
				|
				span #{$text_span}
				if $nameType == "text"
						input(
								type= $nameType,
								placeholder= `${$textplace}`
								value=""
						)
				else if $nameType == "date"
						input(
								type= $nameType,
								value=""
						)
				else if $nameType == "number"
						input(
								type= $nameType,
								placeholder= `${$textplace}`
								value=""
						)
