mixin box_item_video($span_text, $h3_text, $a_text)
		- var styleBtn = "#book-gorm"
		video(src="../video/video-tour-2.mp4" type="video/mp4" autoplay loop muted) 
		div(class= "content")
				div(class="info-title-tour")|
						|
						span= `${$span_text}`
						h3= `${$h3_text}`
						|
						p() Lorem ipsum dolor sit amet consectetur adipisicing elit. Magni exercitationem assumenda odio iusto, quo veniam qui inventore! Eligendi illo 
						|
						|
						a(href= "#", class= "btn") #{styleBtn.replace(`${styleBtn}`,`${$a_text}`)}
				div(class="contact-us-box")
						nav(class="contact-link")
								-
										const $link_contact_us = [
												'fa fa-facebook-f', 
												'fa fa-twitter',
												'fa fa-instagram'
										]
								-for(let $run = 0; $run < $link_contact_us.length; $run++)
										|
										|
										a(href="#")
												i(class=`${$link_contact_us[$run]}`)
